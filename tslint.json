{
    "extends": "tslint:recommended",
    "rules": {
      "max-line-length": {
        "options": [130]
      },
      "member-ordering": false,
      "no-consecutive-blank-lines": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "quotemark": [true, "single"],
      "variable-name": [true, "allow-leading-underscore"],
      "no-console": true,
      "indent": [
        true,
        "4",
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        true,
        "unix"
      ],
      "eol-last": [
        "error"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "lodash/import-scope": [
        "error",
        "member"
      ],
      "lodash/prop-shorthand": [
        "error",
        "never"
      ],
      "lodash/matches-prop-shorthand": [
        "error",
        "never"
      ],
      "interface-name": false,
      "comma-dangle": ["error", {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }]
    }
  }